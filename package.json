{
  "name": "my-next-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "develop": "next dev",
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && node server.js",
    "build-portal-css": "node scripts/compile-portal-css.js",
    "build-biennial-css": "node scripts/compile-biennial-css.js",
    "build-webp": "node scripts/generate-webp.js",
    "postbuild": "node scripts/generateBiennialSitemap.js",
    "build": "npm run build-webp && npm run build-portal-css && npm run build-biennial-css && export NODE_OPTIONS=--openssl-legacy-provider && next build",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && NODE_ENV=production node server.js",
    "deploy": "next build && next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "html-entities": "^2.4.0",
    "jquery": "^3.7.1",
    "moment": "^2.29.1",
    "next": "^11.0.0",
    "next-sitemap": "^4.1.3",
    "nookies": "^2.5.2",
    "pg": "^8.7.1",
    "qs": "^6.10.1",
    "query-string": "^7.1.1",
    "react": "17.0.0",
    "react-bootstrap": "^2.1.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "17.0.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-lazyload": "^3.2.0",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-markdown": "^7.1.2",
    "react-modal": "^3.14.4",
    "react-moment": "^1.1.1",
    "react-router-dom": "^6.3.0",
    "react-slick": "^0.28.1",
    "react-text-fun": "^1.0.0",
    "react-transition-group": "^4.4.2",
    "sass": "^1.45.0",
    "slugify": "^1.6.6",
    "strapi-plugin-populate-deep": "^0.1.2",
    "three": "^0.181.2"
  },
